<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <RECORD>
  <FIELD ID="1" xsi:type="CharTerm" TERMINATOR="\t|\t"/>
  <FIELD ID="2" xsi:type="CharTerm" TERMINATOR="\t|\t"/>
  <FIELD ID="3" xsi:type="CharTerm" TERMINATOR="\t|\t"/>  
  <FIELD ID="4" xsi:type="CharTerm" TERMINATOR="\r\n"/>
 </RECORD>
 <ROW>
  <COLUMN SOURCE="1" NAME="SeqTypeID" xsi:type="SQLTINYINT"/>
  <COLUMN SOURCE="2" NAME="MoleculeType" xsi:type="SQLVARYCHAR" LENGTH="100"/>
  <COLUMN SOURCE="3" NAME="GeneType" xsi:type="SQLVARYCHAR" LENGTH="100"/>
  <COLUMN SOURCE="4" NAME="GeneName" xsi:type="SQLVARYCHAR" LENGTH="100"/>
 </ROW>
</BCPFORMAT>